{
    "name": "gosia-html",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "classnames": "2.2.6",
        "lodash": "4.17.11",
        "node-sass": "4.11.0",
        "react": "16.8.1",
        "react-dom": "16.8.1",
        "react-redux": "6.0.1",
        "react-scripts": "2.1.5",
        "redux": "4.0.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "make-it-pretty": "yarn prettier --config .prettierrc.json --write 'src/**/*.{jsx,js}'"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "ie >= 9",
        "not op_mini all"
    ],
    "devDependencies": {
        "eslint": "5.12.0",
        "eslint-config-airbnb": "17.1.0",
        "eslint-plugin-react": "7.12.4",
        "husky": ".3.1",
        "lint-staged": "8.1.4",
        "prettier": "1.16.4",
        "redux-devtools": "3.5.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    }
}
